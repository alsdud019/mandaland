{
    "semi": false,
    "overrides": [
        {
            "files": ["*.js", "*.jsx", "*.json"],
            "options": {
                "trailingComma": "es5",
                "tabWidth": 4,
                "singleQuote": false,
                "printWidth": 250
            }
        },
        {
            "files": ["*.html", "*.css"],
            "options": {
                "tabWidth": 4,
                "printWidth": 250
            }
        }
    ]
}
